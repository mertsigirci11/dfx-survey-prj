@questionId2 = 65dac648-84d3-47e4-b584-1763daf4e4ab

### Get question detail
GET http://localhost:8081/admin/questions/{{questionId}}
Authorization: Bearer {{JWT_TOKEN}}

### Get question summary
GET http://localhost:8081/admin/questions/{{questionId}}/summary
Authorization: Bearer {{JWT_TOKEN}}

### Edit question
PUT http://localhost:8081/admin/questions/{{questionId}}
Content-Type: application/json
Authorization: Bearer {{JWT_TOKEN}}

{
  "question": "Updated question text",
  "type": "LIKERT",
  "order": 2,
  "options": ["Strongly Dislike", "Dislike", "Neutral", "Like", "Strongly Like"]
}

### Delete question
DELETE http://localhost:8081/admin/questions/{{questionId}}
Authorization: Bearer {{JWT_TOKEN}}

### Duplicate question
POST http://localhost:8081/admin/questions/{{questionId}}/duplicate
Authorization: Bearer {{JWT_TOKEN}}

### Add question to survey
# @name addQuestion
POST http://localhost:8081/admin/questions/{{SURVEY_ID}}
Content-Type: application/json
Authorization: Bearer {{JWT_TOKEN}}

{
  "question": "New question for survey",
  "type": "FREETEXT",
  "order": 2,
  "options": ["What is your favorite animal ?"]
}

#######################################################################
# SURVEY CONTROLLER
#######################################################################

### Create survey
# @name createSurvey
POST http://localhost:8081/admin/surveys
Content-Type: application/json
Authorization: Bearer {{JWT_TOKEN}}

{
  "title": "Customer Satisfaction Survey",
  "validUntil": "2030-12-31T23:59:59"
}

### Get survey detail
GET http://localhost:8081/admin/surveys/{{SURVEY_ID}}
Authorization: Bearer {{JWT_TOKEN}}

### Edit survey
PUT http://localhost:8081/admin/surveys/{{SURVEY_ID}}
Content-Type: application/json
Authorization: Bearer {{JWT_TOKEN}}

{
  "title": "Updated Survey Title",
  "validUntil": "2031-01-01T00:00:00"
}

### Delete survey
DELETE http://localhost:8081/admin/surveys/{{SURVEY_ID}}
Authorization: Bearer {{JWT_TOKEN}}

### Duplicate survey
POST http://localhost:8081/admin/surveys/{{SURVEY_ID}}/duplicate
Authorization: Bearer {{JWT_TOKEN}}

### Send survey
POST http://localhost:8081/admin/surveys/{{SURVEY_ID}}/send
Authorization: Bearer {{JWT_TOKEN}}

### If `send` returns a participant token or a participant list, bind token here (optional)
# @participantToken = {{send.response.body.data.participantToken}}
@participantEmail = aburakkontas@hotmail.com
@participantEmail2 = burakkontass@gmail.com

### Get survey list (paged)
GET http://localhost:8081/admin/surveys/list?pageNumber=0
Authorization: Bearer {{JWT_TOKEN}}

### Get participants of survey
GET http://localhost:8081/admin/surveys/participants/{{SURVEY_ID}}
Authorization: Bearer {{JWT_TOKEN}}

### Add participant to survey
POST http://localhost:8081/admin/surveys/participants
Authorization: Bearer {{JWT_TOKEN}}
Content-Type: application/json

{
    "email": "{{participantEmail}}",
    "id": "{{SURVEY_ID}}"
}

### Delete participant
DELETE http://localhost:8081/admin/surveys/participants/{{SURVEY_ID}}/{{participantEmail}}
Authorization: Bearer {{JWT_TOKEN}}


